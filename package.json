{
  "name": "vd-web",
  "version": "1.0.0",
  "description": "vd-web",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "dependencies": {
    "@types/bootstrap": "^4.3.1",
    "@types/hammerjs": "^2.0.36"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && npm run copy-html",
    "push": "git add -A && git commit -m \"Setup Package\" &&  git push",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A dist",
    "postversion": "git push && git push --tags",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-html": "copyfiles -u 1 ./src/**/*.html ./dist/"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://MartinRiksten:0eb089794c83488984c8c2e70c74c9bbd79a1607@github.com/MartinRiksten/vd-web.git"
  },
  "keywords": [],
  "author": "Martin Riksten",
  "bugs": {
    "url": "https://github.com/MartinRiksten/vd-web/issues"
  },
  "homepage": "https://github.com/MartinRiksten/vd-web#readme",
  "devDependencies": {
    "@jspm/core": "1.0.4",
    "aurelia-dialog": "^2.0.0-rc.8",
    "aurelia-fetch-client": "^1.8.2",
    "aurelia-framework": "^1.3.1",
    "aurelia-templating": "^1.10.2",
    "aurelia-validation": "^1.5.0",
    "bootstrap": "^4.3.1",
    "copyfiles": "^2.1.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  }
}
